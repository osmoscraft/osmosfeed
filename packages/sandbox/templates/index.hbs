<!DOCTYPE html>
<html lang="en">

  <head>
    <title>{{siteTitle}}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
    <link rel="alternate" type="application/rss+xml" title="{{siteTitle}}" href="{{feedHref}}" />
    <link rel="stylesheet" href="index.css?v={{staticDirHash}}" />
    <script type="module" defer src="index.js?v={{staticDirHash}}"></script>
    {{> beforeHeadEnd }}
  </head>

  <body>
    {{> afterBodyBegin }}
    {{> header }}
    <div class="site-layout">
      <div class="date-list">
      {{#each dates}}
        <section class="date-item">
          <ul class="date-item__summary">
            <h2 class="">
              <time class="" data-offset-date={{isoOffsetPublishDate}} datetime="{{isoUtcPublishTime}}">{{isoOffsetPublishDate}}</time>
            </h2>
            <ul>
            {{#each feeds}}
              <li class="">
               <section class="">
                 <div class="">
                  <img src="{{icon}}" width="16" height="16" class="" />
                  <a href="{{home_page_url}}">{{title}}</a>
                </div>
                </section>
                <ul>
                {{#each items}}
                  <li>
                    <article class="">
                      {{#if _extIcon}}
                        <img src="{{_extIcon}}" width="16" height="16" class="" />
                      {{/if}}
                      <a class="" href="{{url}}">{{title}}</a>
                    </article>
                  </li>
                {{/each}}
                </ul>
              </li>
            {{/each}}
            </ul>
          </ul>
          <ul class="feed-list">
            {{#each feeds}}
              {{#each items}}
                <article class="article-item">
                    {{#if _extIcon}}
                      <img src="{{_extIcon}}" width="16" height="16" class="" />
                    {{/if}}
                    <div>
                      <h4 class="">
                        <a class="" href="{{url}}">{{title}}</a>
                      </h4>
                      <details>
                        <summary class="article-item__summary">{{summary}}</summary>
                      </details>
                    </div>
                    {{#if image }}
                      <img src="{{image}}" loading="lazy" width="120" height="auto" class="" />
                    {{/if}}
                </article>
              {{/each}}
            {{/each}}
          </ul>
        </section>
      {{/each}}
      </div>
      <ul class="date-outline-list">
        {{#each dates}}
          <li class="date-item">
            <time class="" data-offset-date={{isoOffsetPublishDate}} datetime="{{isoUtcPublishTime}}">{{isoOffsetPublishDate}}</time>
          </li>
        {{/each}}
      </ul>
      </div>
    </div>
    {{> footer}}
    {{> beforeBodyEnd }}
  </body>

</html>
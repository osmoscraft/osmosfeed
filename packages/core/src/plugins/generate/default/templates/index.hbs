<!DOCTYPE html>
<html lang="en">

  <head>
    <title>{{siteTitle}}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
    <link rel="alternate" type="application/rss+xml" title="{{siteTitle}}" href="{{feedHref}}" />
    <link rel="stylesheet" href="index.css?v={{staticDirHash}}" />
    <script type="module" defer src="index.js?v={{staticDirHash}}"></script>
    {{> beforeHeadEnd }}
  </head>

  <body>
    {{> afterBodyBegin }}
    {{> header }}
    {{#each dates}}
      <section class="">
        <h2 class="">
          <time class="" data-offset-date={{isoOffsetPublishDate}} datetime="{{isoUtcPublishTime}}">{{isoOffsetPublishDate}}</time>
        </h2>
        <ul class="">
          {{#each feeds}}
            <li class="">
              <section class="">
                {{#if icon }}
                  <img src="{{icon}}" loading="lazy" width="16" height="16" class="" />
                {{/if}}
                <h3 class="">{{title}}</h3>
                <a class="" href="{{siteUrl}}">{{siteUrl}}</a>
                <section class="">
                  {{#each items}}
                    <article>
                        <h4 class="">
                          <a class="" href="{{link}}">{{title}}</a>
                        </h4>
                        {{#if _extIcon}}
                          <img src="{{_extIcon}}" width="16" height="16" class="" />
                        {{/if}}
                        {{#if image }}
                          <img src="{{image}}" loading="lazy" width="100" height="auto" class="" />
                        {{/if}}
                        <span>{{summary}}</span>
                        </a>
                    </article>
                  {{/each}}
                </section>
              </section>
            </li>
          {{/each}}
        </ul>
      </section>
    {{/each}}
    {{> footer}}
    {{> beforeBodyEnd }}
  </body>

</html>
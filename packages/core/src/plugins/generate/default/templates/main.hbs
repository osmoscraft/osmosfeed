{{#each dates}}
  <section class="">
    <h2 class="">
      <time class="" data-offset-date={{isoOffsetPublishDate}} datetime="{{isoUtcPublishTime}}">{{isoOffsetPublishDate}}</time>
    </h2>
    <ul class="">
      {{#each feeds}}
        <li class="">
          <section class="">
            <h3 class="">{{title}}</h3>
            <a class="" href="{{siteUrl}}">Open</a>
            <section class="">
              {{#each items}}
                <article>
                    <h4 class="">{{title}}</h4>
                    <a class="" href="{{link}}">
                      {{#if imageUrl}}
                        <img src="{{imageUrl}}" loading="lazy" class="media-object__media article-image" />
                      {{/if}}
                      <span>{{description}}</span>
                      {{#if readingTimeInMin}}
                        &ensp;<span class="article-reading-time">(&hairsp;{{readingTimeInMin}}
                          min&hairsp;)</span>
                      {{/if}}
                    </a>
                </article>
              {{/each}}
            </section>
          </section>
        </li>
      {{/each}}
    </ul>
  </section>
{{/each}}